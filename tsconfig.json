{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"moduleResolution": "Bundler",
		"esModuleInterop": true,
		"rootDir": "./src",

		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"noEmit": false,
		"outDir": "dist",
		"declaration": true
	},
	"include": ["src/**/*"]
}
